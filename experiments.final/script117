#!/bin/bash
set -x
OUT=`pwd`/experiments.final/output17

for psi in 0 1 ; do
    for alpha in 0 0.4 0.8 1.2 1.6 2 2.4 2.8 3 ; do
	OUTDIR=$OUT/f2_psi${psi}_alpha${alpha}
	mkdir -p ${OUTDIR}_l
        for layers in "50_50_1" "70_70_70_1" ; do
				    python biholoNN_train.py --seed 1234 \
				 --n_pairs 20000 \
				 --optimizer lbfgs \
		          	 --function "f2" \
				   --psi $psi \
                                     --alpha $alpha \
				 --layers $layers \
				 --load_model $OUTDIR/$layers \
				 --save_dir ${OUTDIR}_l/ \
 				--save_name $layers \
				 --max_epochs 500 \
				 --loss_func "weighted_MSE" 
        done
    done
done
